{
  "name": "@breadhead/s3-uploader",
  "version": "1.2.1",
  "main": "dist/index.js",
  "repository": "git@github.com:breadhead/s3-uploader.git",
  "author": "Igor Kamyshev <igor@kamyshev.me>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare": "yarn build",
    "ci": "yarn types && yarn lint",
    "lint": "tslint {lib}/**/*.ts -c tslint.json",
    "types": "tsc --noEmit",
    "pretty": "prettier --write \"lib/**/*.ts\"",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@solid-soda/tslint-config": "^2.5.3",
    "@types/jest": "^24.0.6",
    "@types/md5": "^2.1.33",
    "@types/minio": "^7.0.1",
    "@types/node": "^11.9.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "standard-version": "^5.0.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn types",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "yarn pretty",
      "yarn lint",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "aws-sdk": "^2.409.0",
    "md5": "^2.2.1",
    "minio": "^7.0.5"
  }
}
